    "enve": "^1.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-persist": "^3.0.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/node-persist": "0.0.33",
